{
    "build": {
        "dockerfile": "Dockerfile",
        "args": {
            "VARIANT": "focal"
        }
    },
    "containerEnv": {
        "MY_LOCAL_HOME": "${localEnv:HOME}",
        "A_SIMPLE_KEY": "A_SIMPLE_VALUE"
    },
    "postCreateCommand": "echo `whoami`> whoami.txt && bash ./run-me.sh"
}
